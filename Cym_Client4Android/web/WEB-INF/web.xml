<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
           version="3.0">
    <servlet>
        <servlet-name>Servlet4AndroidLogin</servlet-name>
        <servlet-class>com.cym.Servlet4Android.Servlet4AndroidLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Servlet4AndroidRegister</servlet-name>
        <servlet-class>com.cym.Servlet4Android.Servlet4AndroidRegister</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Servlet4GetStudent</servlet-name>
        <servlet-class>com.cym.Servlet4Android.Servlet4GetStudent</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servlet4LoadPictures</servlet-name>
        <servlet-class>com.cym.Servlet4Android.Servlet4LoadPictures</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>servlet4LoadPictures</servlet-name>
        <url-pattern>/getImage.jpeg</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servlet4AndroidLogin</servlet-name>
        <url-pattern>/login.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servlet4AndroidRegister</servlet-name>
        <url-pattern>/register.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servlet4GetStudent</servlet-name>
        <url-pattern>/getStudent.do</url-pattern>
    </servlet-mapping>

</web-app>
